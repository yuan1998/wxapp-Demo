page{
	font: 14px/1.5 "Helvetica Neue",Helvetica,Arial,"Microsoft Yahei","Hiragino Sans GB","Heiti SC","WenQuanYi Micro Hei",sans-serif;
	background-color: #fff;
}

.center{
	text-align:center;
}

.flex-wra{
	display: flex;
}

.flex-col{
	flex-direction: column;
}

.flex-row{
	flex-direction: row;
}
.flex-center{
	justify-content: center;
}
.flex-between{
	justify-content: space-between;
}

.col{
	flex : 1 1 auto;
}

.col-1{
	flex: 1 1 8.333333%;
}
.col-2{
	flex: 1 1 16.666666%;
}
.col-3{
	flex: 1 1 24.99999%;
}
.col-4{
	flex: 1 1 33.333333%;
}

.col-5{
	flex: 1 1 41.6666666%;
}
.col-6{
	flex: 1 1 50%;
}
.col-7{
	flex: 1 1 58.3333333%;
}
.col-8{
	flex: 1 1 66.666666%;
}
.col-9{
	flex: 1 1 74.99999999%;
}
.col-10{
	flex: 1 1 83.3333333%;
}
.col-11{
	flex: 1 1 91.6666666%;
}
.col-12{
